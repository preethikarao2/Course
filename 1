Find diiference between maximumcount of elements in an array and minimum count of elements in an array
 intput:
 n-6
 array- 2 4 5 4 4 5
 
 output:
 no.of times repeated element 4 is 3
 no.of times repeated element 5 is 2
 no.of times repeated element 2 is 1
 
 so max is 3 and min is 2
 diff=max-min=3-1 =2
 
 
 
 import java.util.*;  
public class Main   
{  
public static void main(String args[])  
{  
int n, digit, sum = 0;  
Scanner sc = new Scanner(System.in); 
n = sc.nextInt();
int arr[]=new int[n];
for(int i=0;i<n;i++){
    arr[i]=sc.nextInt();
}
 
  
 HashMap<Integer,Integer> h=new HashMap<>();
 for(int i=0;i<n;i++){
     h.put(arr[i],h.getOrDefault(arr[i],0)+1);
 }
 
        int maxValueInMap = (Collections.max(h.values()));
        
        int minValueInMap = (Collections.min(h.values()));
        System.out.println(maxValueInMap-minValueInMap);
}  
}  
 
